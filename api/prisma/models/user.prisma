model User {
  id              Int        @id @default(autoincrement())
  name            String
  email           String     @unique
  password        String
  image           String?
  emailVerifiedAt DateTime?
  phone           String? 
  rememberToken   String?
  isAdmin         Int         @default(0)
  status          Int         @default(1)
  createdAt       DateTime    @default(now())
  updatedAt       DateTime    @updatedAt
}
